package example1;


class Person{
	String name;
	int age = 1;
	
	public void Person(String name , int age){
		
		this.name = name;
		this.age = age;
		
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
}

public class Application {
	
	public static void main (String [] args){
		
		Person p = new Person();
		 int i = p.getAge();
		System.out.println(i);
		
	}
	

}
